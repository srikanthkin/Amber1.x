	.stylish-panel > div > div
	{
    		padding: 10px;
    		border: 1px solid transparent;
    		border-radius: 4px;
		box-shadow: 2px 2px 2px 0 rgba(0,0,0,0.6);
    		transition: 0.5s;
	}

	.stylish-panel > div:hover > div
	{
    		margin-top: -10px;
    		transition: 0.9s;
	}

	.stylish-panel > div:hover img 
	{
    		border-radius: 50%;
    		transform: rotate(360deg);
    		transform: rotate(360deg);
    		transform: rotate(360deg);
    		transform: rotate(360deg);
    		transform: rotate(360deg);
	}

	.fa-5x
