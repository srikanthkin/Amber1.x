                        function formValidations()
                        {

                                var empid = document.getElementById("empid").value;
                                if(empid != "")
                                {
                                        var empid = document.getElementById("empid").value;
                                        if(empid.length < 4)
                                        {
                                                document.getElementById("empid").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Enter a Valid Employee Id"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;
                                        }

                                        var validsen = /^\d{1,4}?$/;
                                        if (validsen.test(empid)==false)
                                        {
                                                //alert("Enter a Valid Employee Id");
                                                document.getElementById("empid").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Enter a Valid Employee Id"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;
                                        }

                                }
                                else
                                {
                                                document.getElementById("empid").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Enter the Employee Id"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;

                                }

                                var empName = document.getElementById("empName").value;
                                if(empName == "")
                                {
                                                //alert("Enter the Employee Name");
                                                document.getElementById("empName").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Enter the Employee Name"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;
                                }
                                else
                                {
                                        if (!/^[a-zA-Z\s]+$/.test(document.getElementById("empName").value))
                                        {
                                                //alert("Employee Name Should Contain Only Letters.");
                                                document.getElementById("empName").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Employee Name Should Contain Only Letters"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;
                                        }

                                }

                                var email = document.forms["myForm"]["email"].value;
                                if(email != "")
                                {
                                        var atpos = email.indexOf("@");
                                        var dotpos = email.lastIndexOf(".");
                                        if (atpos<1 || dotpos<atpos+2 || dotpos+2>=email.length)
                                        {
                                                //alert("Not a valid E-Mail address");
                                                document.getElementById("email").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Not a valid E-Mail address"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;
                                        }

                                        if(email.indexOf('@nurture\.co\.in') < 0)
                                        {
                                                //alert("Not a valid Nurture E-Mail address");
                                                document.getElementById("email").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Not a valid Nurture E-Mail address"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;

                                        }

                                }
                                else
                                {

                                                //alert("Enter The Email Address");
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'fa fa-exclamation-circle',
                                                message: "Enter The Email Address"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                document.getElementById("email").focus();
                                                return false;

                                }

                                if(jDate == "")
                                {
                                              //alert("Enter the Joining Date");
                                              document.getElementById("jDate").focus();
                                                demo.initChartist();
                                                $.notify({
                                                icon: 'pe-7s-gift',
                                                message: "Enter the Joining Date"
                                                },{
                                                type: 'info',
                                                timer: 500
                                                });
                                                return false;
                                }

                                var test = document.querySelector('input[name = "preem"]:checked').value;
                                if(test == "Y")
                                {

                                        var preExpyear = document.getElementById("preExpyear").value;
                                        if(preExpyear == "")
                                        {
                                                 //alert("Enter the Previous Experience in Years");
                                                 document.getElementById("preExpyear").focus();
                                                 demo.initChartist();
                                                 $.notify({
                                                 icon: 'fa fa-exclamation-circle',
                                                 message: "Enter the Previous Experience in Years"
                                                 },{
                                                 type: 'info',
                                                 timer: 500
                                                 });
                                                 return false;

                                        }
                                        else
                                        {

                                                if (isNaN(preExpyear))
                                                {
                                                        //alert("Enter a Valid Previous Experience in Years");
                                                        document.getElementById("preExpyear").focus();
                                                        demo.initChartist();
                                                        $.notify({
                                                        icon: 'fa fa-exclamation-circle',
                                                        message: "Enter a Valid Previous Experience in Years"
                                                        },{
                                                        type: 'info',
                                                        timer: 500
                                                        });
                                                        return false;
                                                }
                                                var preExpmonth = document.getElementById("preExpmonth").value;
                                                var preEmp = document.getElementById("preEmp").value;
                                                if (( preExpyear == 0 )||( preExpyear >= 1 ))
                                                {
                                                        if(preExpmonth == "")
                                                        {
                                                                //alert("Enter The Previous Employment in Months");
                                                                document.getElementById("preExpmonth").focus();
                                                                demo.initChartist();
                                                                $.notify({
                                                                icon: 'fa fa-exclamation-circle',
                                                                message: "Enter The Previous Employment in Months"
                                                                },{
                                                                type: 'info',
                                                                timer: 500
                                                                });
                                                                return false;
                                                        }
                                                }

                                                if (( preExpyear >= 1 ) || (preExpmonth > 0))
                                                {
                                                        if(preEmp == "")
                                                        {
                                                                //alert("Enter The Previous Employer In Chronological Order");
                                                                document.getElementById("preEmp").focus();
                                                                demo.initChartist();
                                                                $.notify({
                                                                icon: 'fa fa-exclamation-circle',
                                                                message: "Enter The Previous Employer In Chronological Order"
                                                                },{
                                                                type: 'info',
                                                                timer: 500
                                                                });
                                                                return false;
                                                        }
                                                        else
                                                        {
                                                                if (!/^[a-zA-Z\s]+$/.test(document.getElementById("preEmp").value))
                                                                {
                                                                        //alert("Previous Employer In Chronological Order Should Contain Only Letters.");
                                                                        document.getElementById("preEmp").focus();
                                                                        demo.initChartist();
                                                                        $.notify({
                                                                        icon: 'fa fa-exclamation-circle',
                                                                        message: "Previous Employer In Chronological Order Should Contain Only Letters"
                                                                        },{
                                                                        type: 'info',
                                                                        timer: 500
                                                                        });
                                                                        return false;
                                                                }
                                                        }
                                                }
                                        }

                                        var preEmp2 = document.getElementById("preEmp2").value;
                                        if(preEmp2 != "")
                                        {

                                                if (!/^[a-zA-Z\s]+$/.test(document.getElementById("preEmp2").value))
                                                {
                                                        //alert("Previous Employer Two Should Contain Only Letters.");
                                                        document.getElementById("preEmp2").focus();
                                                        demo.initChartist();
                                                        $.notify({
                                                        icon: 'fa fa-exclamation-circle',
                                                        message: "Previous Employer Two Should Contain Only Letters"
                                                        },{
                                                        type: 'info',
                                                        timer: 500
                                                        });
                                                        return false;
                                                }
                                        }


                                        var preEmp3 = document.getElementById("preEmp3").value;
                                        if(preEmp3 != "")
                                        {

                                                if (!/^[a-zA-Z\s]+$/.test(document.getElementById("preEmp3").value))
                                                {
                                                        //alert("Previous Employer Three Should Contain Only Letters.");
                                                        document.getElementById("preEmp3").focus();
                                                        demo.initChartist();
                                                        $.notify({
                                                        icon: 'fa fa-exclamation-circle',
                                                        message: "Previous Employer Three Should Contain Only Letters"
                                                        },{
                                                        type: 'info',
                                                        timer: 500
                                                        });
                                                        return false;
                                                }
                                        }


                                        var preEmp4 = document.getElementById("preEmp4").value;
                                        if(preEmp4 != "")
                                        {

                                                if (!/^[a-zA-Z\s]+$/.test(document.getElementById("preEmp4").value))
                                                {
                                                        //alert("Previous Employer Four Should Contain Only Letters.");
                                                        document.getElementById("preEmp4").focus();
                                                        demo.initChartist();
                                                        $.notify({
                                                        icon: 'fa fa-exclamation-circle',
                                                        message: "Previous Employer Four Should Contain Only Letters"
                                                        },{
                                                        type: 'info',
                                                        timer: 500
                                                        });

                                                        return false;
                                                }
                                        }

                                        var preEmp5 = document.getElementById("preEmp5").value;
                                        if(preEmp5 != "")
                                        {

                                                if (!/^[a-zA-Z\s]+$/.test(document.getElementById("preEmp5").value))
                                                {
                                                        //alert("Previous Employer Five Should Contain Only Letters.");
                                                        document.getElementById("preEmp5").focus();
                                                        demo.initChartist();
                                                        $.notify({
                                                        icon: 'fa fa-exclamation-circle',
                                                        message: "Previous Employer Five Should Contain Only Letters"
                                                        },{
                                                        type: 'info',
                                                        timer: 500
                                                        });
                                                        return false;
                                                }
                                        }

                                }
                                return true;
                        }

                        function showId()
                        {
                                var preem = document.querySelector('input[name = "preem"]:checked').value;
                                if(preem == "Y")
                                {

                                        document.getElementById("yes").style.display = "block";
                                }
                                else
                                {
                                        document.getElementById("yes").style.display = "none";


                                }
                        }



			function resetform()
			{

				document.getElementById("myForm").reset();
			}


			function dontAllow()
			{
				demo.initChartist();
				$.notify({
				icon: 'fa fa-exclamation-circle',
				message: "Please fill out this tab in order to move to the next one"
				},{
				type: 'info',
				timer: 500
				});
				return false;

			}

	    </script>

