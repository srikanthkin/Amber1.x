	     <script type="text/javascript">
                        
			$(window).load(function()
                        {
                                $('#jDate').pickadate(
                                {
                                max:true,
                                selectMonths: true,
                                selectYears: 100,
                                closeOnSelect: true,
                                closeOnClear: true,
                                format: 'yyyy-mm-dd',
                                formatSubmit: 'yyyy-mm-dd'
                                });
                        });

                        $(document).ready(function() {
                        $("#my-icon").click(function(){
                        $( '#jDate' ).pickadate("open"),
                        event.stopPropagation(),
                        event.preventDefault()
                        });
