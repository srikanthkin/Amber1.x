         <script type="text/javascript">

         $(document).ready(function() {
            
            $(".confirm").attr('disabled',true);
            $("#costChk").hide();
            $("#costChkSucc").hide();
            var cost=$( "#free_text_1" ).val();
            
            $("#chkbudget").click(function() {
                 var cost=parseInt($( "#free_text_1" ).val());

                 var project_id=$( "#project_id" ).val();
                var approver_id=$("#approver_id").val();

                var emp_access=$("#emp_access").val();
                var amount_refunded=$("#amount_refunded").val();
				var modify_flg=$("#modify_flg").val();
					var prevCost=$("#prevCost").val();
                 if(cost=="" || isNaN(cost))
                 {
                demo.initChartist();

          $.notify({
              icon: 'pe-7s-gift',
              message: "Enter the cost"

            },{
                type: 'info',
                timer: 500
            });
 
                 }
                else{  
            
         
            $.ajax
         ({
             type:'post',
             url:'/travelModule/approveReq/checkbudget',

             data:{
                cost:cost,
                project_id:project_id,
                approver_id:approver_id,
                emp_access:emp_access,
				amount_refunded:amount_refunded,
                prevCost:prevCost,
				modify_flg:modify_flg
				
             },
             success:function(response,data)
             {
                console.log("response",response);
                //console.log("cost",cost);
                if(cost>parseInt(response.key))
                {
                  $(".confirm").attr('disabled',true); 
                  $("#costChk").show();
				          $("#costChkSucc").hide();
						      $("#available_budget").val(response.key);
                 }
                 else if(cost<=parseInt(response.key))
                 {
					 $(".confirm").attr('disabled',false); 
					        $("#costChk").hide();
                  $("#costChkSucc").show();
                 }
                  $("#free_text_1").val(cost);     
                 } 
               
               
             });

       }
     
     
      });    

});

            
             
            
