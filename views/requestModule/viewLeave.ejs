					<td style="text-align: left;"> <%= leaveData[i].comm_code_desc %></td>
    					<td style="text-align: left;"> <%= leaveData[i].from_date.getFullYear() %>-<%= leaveData[i].from_date.getMonth()+1 %>-<%= leaveData[i].from_date.getDate() %></td>
    					<td style="text-align: left;"> <%= leaveData[i].to_date.getFullYear() %>-<%= leaveData[i].to_date.getMonth()+1 %>-<%= leaveData[i].to_date.getDate() %></td>
   					<td style="text-align: left;"> <%= leaveData[i].reason %></td>
   					<td style="text-align: left;"> <%= leaveData[i].emp_name %></td>
   					<td> <%= leaveData[i].availed_leaves %></td>

					<% if(leaveData[i].rej_flg == 'N') { %>
   					<% if(leaveData[i].app_flg == 'N') { %>
   					<td>Pending</td>
   					<% } else if(leaveData[i].app_flg == 'Y') { %>
   					<td>Approved</td>
   					<% } %>
   					<% } else if(leaveData[i].rej_flg == 'Y') { %>
   					<td>Rejected</td>
   					<% } %>
   				
					<% if(leaveData[i].rej_flg == 'N' &&  leaveData[i].app_flg == 'N') { %>
