<script>
     $(function() {     
    $('#paid').on('click' , function() {
         var settleAmt=$("#settlement_Amount").val();
        var remb_id=$("#remb_id").val();
        if(settleAmt=="")
        {
          document.getElementById("settlement_Amount").focus();
             demo.initChartist();
                        
                          $.notify({
                          icon: 'fa fa-exclamation-circle',
                          message: "Enter the settlement amount"

                          },{
                          type: 'info',
                          timer: 500
                          });
            return false; 
          }
          if(isNaN(settleAmt)||parseInt(settleAmt)<=0)
          {
            document.getElementById("settlement_Amount").focus();
             demo.initChartist();
                        
                          $.notify({
                          icon: 'fa fa-exclamation-circle',
                          message: "Enter valid settlement amount"

                          },{
                          type: 'info',
                          timer: 500
                          });
            return false; 
          }
       
    var settlermks=$('#settlementtRemarks').val();
    if(settlermks=="")
    {
    document.getElementById("settlementtRemarks").focus();
             demo.initChartist();
                        
                          $.notify({
                          icon: 'fa fa-exclamation-circle',
                          message: "Enter the remarks for settlement amount variation "

                          },{
                          type: 'info',
                          timer: 500
                          });
            return false; 
          }

      
        var net_amt_payable=$("#net_amt_payable").val();
        var advance_amt =$("#advAmt").val();
        var claimAmt=parseInt(net_amt_payable)-parseInt(advance_amt);
        console.log(claimAmt);
        var settleAmt=$("#settlement_Amount").val();
        if(parseInt(claimAmt)<parseInt(settleAmt))
        {
           document.getElementById("settlement_Amount").focus();
             demo.initChartist();
                        
                          $.notify({
                          icon: 'fa fa-exclamation-circle',
                          message: "Settlement Amount cannot be greater than claim amount "

                          },{
                          type: 'info',
                          timer: 500
                          });
            return false; 
        }
      
         return true; 
    })
});

      </script>





