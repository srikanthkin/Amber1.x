                                                <label style="color:#404040;margin-left: 50px">Employee Id <span style="color:red;font-size: 10pt">*</span></label>
                                                <input type="text" style ="margin-left: 50px;width:348px" id="empid" maxlength="4" name="empid" class="form-control" placeholder="Employee Id">
                                            </div>
                                        </div>


					<div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Employee Name <span style="color:red;font-size: 10pt">*</span></label>
                                                <input type="text" style ="margin-left: 50px;width:348px" id="empName" name="empName" class="form-control" placeholder="Employee Name" maxlength="99">
                                            </div>
                                        </div>
                                      </div>

                                    <div class="row">
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Email <span style="color:red;font-size: 10pt">*</span></label>
                                                <input type="text" style ="margin-left: 50px;width:348px" maxlength="50" id="email" name="email" class="form-control" placeholder="Email">
                                            </div>
                                        </div>
					
					<div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Employee Access <span style="color:red;font-size: 10pt">*</span></label>
                                                <br>
                                                <select class="form-control" id="empAccess" name="empAccess" style ="margin-left: 50px;width:348px">
                                                        <option value="" disabled selected>Select The Employee Access</option>
							<% for (i = 0; i < comm_code_id_count; i++)  {%>
                                                        <option value="<%= comm_code_id[i].comm_code_id %>"> <%= comm_code_desc[i].comm_code_desc %></option>
                                                        <% }; %>


                                              </select>
                                            </div>
                                        </div>
                                    </div>
                                   
                                <div class="row">
					    <div class="col-md-5">
                                            <div class="form-group">
                                            <div class="datepicker">
                                            <table>
                                                <tr><label style="color:#404040;margin-left: 50px">Joining Date  (YYYY-MM-DD) <span style="color:red;font-size: 10pt">*</span></label>
                                                <td><input type="text" readonly class="form-control" style ="margin-left: 50px;width:348px" placeholder="Date of Joining" id="jDate" name="jDate"></td>
                                                <td><span id="my-icon" class="glyphicon glyphicon-calendar"></span></td>
                                            </tr>
                                            </table>
                                                </div>
                                            </div>
                                        </div>
 
                                    
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Designation <span style="color:red;font-size: 10pt">*</span></label>
                                            <select class="form-control" id="desig" name="desig" style ="margin-left: 50px;width:348px" onchange="return showGrade();">
                                                        <option value="" disabled selected>Select The Employee Designation</option>
							<% for (i = 0; i < desig_code_id_count; i++)  {%>
                                                        <option value="<%= desig_code_id[i].comm_code_id %>"> <%= desig_code_desc[i].comm_code_desc %></option>
                                                        <% }; %>

                                                </select>
					    </div>
                                        </div>
                                </div>
				
				<div class="row">
					<div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Employee Classification <span style="color:red;font-size: 10pt">*</span></label>
							<select class="form-control" id="empClass" name="empClass" style ="margin-left: 50px;width:348px" maxlength="2">
                                                        <option value="" disabled selected>Select The Employee Classification</option>
                                                        <option value="A1">A1</option>
                                                        <option value="A2">A2</option>
                                                        <option value="B1">B1</option>
                                                        <option value="B2">B2</option>
                                                        <option value="C1">C1</option>
                                                        <option value="C2">C2</option>
                                                        <option value="D1">D1</option>
                                                        <option value="D2">D2</option>
                                                        <option value="E1">E1</option>
                                                        <option value="E2">E2</option>
                                                        <option value="F1">F1</option>
                                                        <option value="F2">F2</option>
                                                        <option value="G1">G1</option>
                                                        <option value="H">H</option>
                                                </select>

                                            </div>
                                        </div>



                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Gross Salary Per Month <span style="color:red;font-size: 10pt">*</span></label>
                                                <input type="text" id="salary" name="salary" maxlength="17" class="form-control" placeholder="Salary" style ="margin-left: 50px;width:348px ;text-align:right"> 
                                            </div>
                                        </div>
                               </div>
                                        
				<div class="row">
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Salary Currency <span style="color:red;font-size: 10pt">*</span></label>
                                            <select class="form-control" id="sal_curr" name="sal_curr" style ="margin-left: 50px;width:348px">
                                                        <option value="" disabled selected>Select the Salary Currency</option>
                                                        <% for (i = 0; i < sal_curr_count; i++)  {%>
                                                        <option value="<%= sal_curr[i].comm_code_id %>"> <%= sal_curr[i].comm_code_id %> - <%= sal_curr[i].comm_code_desc %></option>
                                                        <% }; %>
                                            </select>
                                            </div>
                                        </div>


					<div class="col-md-5">
                                            <div class="form-group">
                                                <label style="color:#404040;margin-left: 50px">Reporting Manager <span style="color:red;font-size: 10pt">*</span></label>
                                            <select class="form-control" id="rptMan" name="rptMan" style ="margin-left: 50px;width:348px">
                                                        <option value="" disabled selected>Select The Reporting Manager</option>
                                                        <% for (i = 0; i < rptMan_count; i++)  {%>
                                                        <option value="<%= rptMan_id[i].emp_id %>"> <%= rptMan_id[i].emp_id %> - <%= rpt_name[i].emp_name %></option>
                                                        <% }; %>
                                            </select>
                                            </div>
                                        </div>
				</div>


				<div class="row">
					<div class="form-group">
  						<div class="col-md-5">
							<label style="color:#404040;margin-left: 50px" for="probPeriod">Probation Period <span style="color:red;font-size: 10pt">*</span></label><br>
							<input style ="margin-left: 50px;" type="radio" name="probPeriod" value="N" checked="checked" /> No&nbsp;&nbsp;
							<input style ="margin-left: 50px;" type="radio" name="probPeriod" value="Y"> Yes
                                        	</div>


                                        <div class="col-md-5">
                                                <label style="color:#404040;margin-left: 50px" >Previous Experience <span style="color:red;font-size: 10pt">*</span></label><br>
                                                <input type="radio" style ="margin-left: 50px;" id="preem0" name="preem" value="N" required onchange="return showId();"> No&nbsp;&nbsp;
                                                <input type="radio" style ="margin-left: 50px;" id="preem1" name="preem" value="Y" onchange="return showId();"> Yes
                                        </div>
                                        </div>
				</div>
				<br>
				<div id="yes">
				<hr>
				<h4 class="title">Experience Details     <i class="material-icons right">work</i></h4>
				<hr>
                                        
				<div class="row">
					<div class="col-md-5">
                                            <div class="form-group">
						<table>
                                                <tr><label style="color:#404040;margin-left: 50px">Previous Experience In (Years And Months) <span style="color:red;font-size: 10pt">*</span></label>
						<td>
                                                <select class="form-control" id="preExpyear" name="preExpyear" style ="margin-left: 50px;width:149px">
                                                <option value="" selected>Years</option>
                                                <% for (i = 1; i < 60; i++)  {%>
                                                <option value="<%= i %>"> <%= i %></option>
                                                <% }; %>
                                                </select>

						</td>

                                                <td>
						<select class="form-control" id="preExpmonth" name="preExpmonth" style ="margin-left: 50px;width:149px">
                                                        <option value="" selected>Months</option>
							<% for (i = 0; i < 12; i++)  {%>
							<option value="<%= i %>"> <%= i %></option>
							<% }; %>
                                                </select>
						</td>
						</tr>
						</table>
                                            </div>
                                        </div>
                                        
					<div class="col-md-5">
                                            <div class="form-group">
                                                <label id="preEmpLabel" style="color:#404040;margin-left: 50px">Previous Employment In Chronological Order</label>
                                                <input type="text" style ="margin-left: 50px;width:348px" id="preEmp" name="preEmp" class="form-control" placeholder="Previous Employer One" maxlength="50">
                                            </div>
                                        </div>
                               	  </div>


                                <div class="row">
                                        <div class="col-md-5">
                                                <div class="form-group">
                                                        <label id="preEmp2Label" style="color:#404040;margin-left: 50px">Previous Employer Two</label>
                                                        <input type="text" style ="margin-left: 50px;width:348px" id="preEmp2" name="preEmp2" class="form-control" placeholder="Previous Employer Two" maxlength="50">
                                                </div>
                                            </div>


                                        <div class="col-md-5">
                                                <div class="form-group">
                                                        <label id="preEmp3Label" style="color:#404040;margin-left: 50px">Previous Employer Three</label>
                                                        <input type="text" style ="margin-left: 50px;width:348px" id="preEmp3" name="preEmp3" class="form-control" placeholder="Previous Employer Three" maxlength="50">
                                                </div>
                                        </div>
                                </div>

                                <div class="row">
                                        <div class="col-md-5">
                                                <div class="form-group">
                                                        <label id="preEmp4Label" style="color:#404040;margin-left: 50px">Previous Employer Four</label>
                                                        <input type="text" style ="margin-left: 50px;width:348px" id="preEmp4" name="preEmp4" class="form-control" placeholder="Previous Employer Four" maxlength="50">
                                                </div>
                                         </div>

                                        <div class="col-md-5">
                                                <div class="form-group">
                                                        <label id="preEmp5Label" style="color:#404040;margin-left: 50px">Previous Employer Five</label>
                                                        <input type="text" style ="margin-left: 50px;width:348px" id="preEmp5" name="preEmp5" class="form-control" placeholder="Previous Employer Five" maxlength="50">
                                                </div>
                                        </div>
                                </div>
                                </div>
				    <br>
				    <table>
                                       <tr>
				       <br>
                                    		<td><input type="submit" name="test" id="test" style="margin-left:50%;padding:1px 16px;height:40px;width:110px" class="btn btn-info btn-fill" style="float: center;" value="Add Profile" onclick="return formValidations()"></td>
	                            		<td><input type="button" style="margin-left:70%;padding:1px 16px;height:40px;width:100px" class="btn btn-info btn-fill" style="float: center;" value="Clear" onclick="return resetform()"></td>
				      </tr>
                                    </table>
                                    <div class="clearfix"></div>
				    <br>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
	<% include ../common/footer %>
</div>

	
