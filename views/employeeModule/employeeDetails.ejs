			function showGrade()
			{
				var desig = document.getElementById("desig").value;
				
				if((desig=="T"))
				{
					document.getElementById("empClass").value="A1";
				}
			
				if((desig=="SE"))
                                {
                                        document.getElementById("empClass").value="A2";
