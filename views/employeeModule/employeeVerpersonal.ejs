                                                        <% for (i = 0; i < comm_code_maritalstatus_count; i++)  {%>
                                                        <% if(maritalstatus != comm_code_maritalstatus[i].comm_code_id) { %>
                                                        <option value="<%= comm_code_maritalstatus[i].comm_code_id %>"> <%= comm_code_maritalstatus[i].comm_code_desc %></option>
                                                        <% } %>
                                                        <% } %>
                                                </select>
                                            </div>
                                        </div>
                                     </div>

                                    <div class="row">
                                        <div class="col-md-5">
