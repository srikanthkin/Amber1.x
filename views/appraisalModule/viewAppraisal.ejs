                {
                        float: left;
                }

                ul.topnav li a
                {
                        display: block;
                        color: white;
                        text-align: center;
                        padding: 14px 16px;
                        text-decoration: none;
                }

                ul.topnav li a:hover:not(.active)
                {
                        background-color: #111;
                }

