        <title>E-Docket</title>

	<style>
		.navbar .navbar-reverse
		{
			display: inline-block;
			float: none;
			vertical-align: top;
		}

		.navbar .navbar-collapse
		{
			text-align: center;
		}

		table, tr, td
		{
		    border: 0px;
		    padding: 2px;
		}

		table
		{
			border-spacing: 0px;
		}

		hr
		{
			border: none;
			height: 1px;
			color: #333;
			background-color: #333;
		}
		 /* visited link */
                a:visited {
                    color: purple;
                }

                /* mouse over link */
                a:hover {
                    color: red;
                    text-decoration: underline;
                }

                /* selected link */
                a:active {
                    color: hotpink;
                }
	</style>

	<script>

		function deletePolicy(doc,policyTag)
		{
			var del = confirm("Are you sure want to delete?");
			if(del == true)
			{
				location.href = "/cmsModule/cms/policyDeleteDocs?doc="+doc+"&policyTag="+policyTag;
			}
		}

	</script>


</head>
<body onload="changeHashOnLoad();">
