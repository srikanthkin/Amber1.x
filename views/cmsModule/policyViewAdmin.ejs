<div class="main-panel">
<div class="container-fluid">
        <% include ../common/userBar %>
<br>

   <div class="content">
      <div class="container-fluid">
         <div class="row">
	    <div class="col">
	       <div class="card">
                  <div class="header2row">
                     <nav class="navbar navbar-inverse" >
                        <ul class="nav navbar-nav" style="color:#669999">
			<li><a href="/cmsModule/cms/cmsUploadAdmin">Upload Documents</a></li>
                        <li><a href="/cmsModule/cms/cmsApprAdmin">Approval Pending</a></li>
                        <li><a href="/cmsModule/cms/cmsViewAdmin">View Documents</a></li>
			<% if(emp_access == "A1"){%>
				<li><a href="/cmsModule/cms/policyUploadAdmin">Upload Policy Docs</a></li>
			<% } %>
			<li><a href="/cmsModule/cms/policyViewAdmin"><span style="color:#ff6600">View Policy Docs</span></a></li>	
			<% if(emp_access == "A1"){%>
                                <li><a href="/cmsModule/cms/magazineUploadAdmin">Upload Magazines</a></li>
                        <% } %>
			<li><a href="/cmsModule/cms/magazineViewAdmin">View Magazines</a></li>
			<li><a href="/cmsModule/cmsFAQDetails/cmsFAQDetails">FAQ</a></li>
			</ul>
		     </nav>
		  </div>
		  <div class="content">
			<form name="policyForm" id="policyForm" action="/cmsModule/cms/policyViewAdminPost" method="post">
			<hr/>
				<h4 class="title">Policy Documents      <i class="fa fa-file" aria-hidden="true"></i></h4>
			<hr/>
				<div class="row">
                                   <div class="col-md-5">
                                      <div class="form-group">
                                         <label style="color:#404040;">Policy Tag <span style="color:red;font-size: 10pt">*</span></label>
					 <select class="form-control" id="policyTag" name="policyTag">
                                             <option value="ALL">ALL</option>
                                             <% for (i = 0; i < policy_count; i++)  {
                                                if(policy[i].comm_code_desc == policyTag) {%>
                                                        <option value="<%=policy[i].comm_code_desc%>" selected><%=policy[i].comm_code_desc%></option>
                                                <%}else{%>
                                                        <option value="<%=policy[i].comm_code_desc%>"><%=policy[i].comm_code_desc%></option>
                                                <%}
                                             };%>
                                         </select>
                                      </div>
                                   </div>
			
				   <div class="col-md-5">
                                      <div class="form-group">
					<label style="color:#404040;font-size: 10pt"></label><br>
                                        <input type="submit" name="btnPolViewAdm" id="btnPolViewAdm" class="btn btn-info btn-fill" style="float: center;" value="View">
                                      </div>
                                   </div>
                                </div>
				
				<% if(polLen != 0) {%>
				   <div class="row">
					<div class="col-sm-6" >
					  <ol>
					      <% for (i = 0; i < polLen; i++)  {%>
						<li><span style="width:100%; word-wrap:break-word; display:inline-block;">
						<a href="/cmsModule/cms/policyViewFilesAdmin?id=<%=polDocs[i]%>" target="_blank"><%=polDocs[i]%></a>
						<% if(emp_access == "A1"){%>
							<a style="color:#293699;" title="Delete" onclick="return deletePolicy(&#39;<%=polDocs[i]%>&#39;,&#39;<%=policyTag%>&#39;)">
							<i class="fa fa-times fa-lg" aria-hidden="true"></i></a>
						<% } %>
						</span></li>
					      <% }; %>
					  </ol>
					</div>
				   </div>
				<% } else { %>
					<label style="color:#404040;">No records found </label>
					<br/>
				<% } %>
				        <div class="clearfix"></div>
                        </form>
		  </div>
               </div>
           </div>
        </div>
     </div>
  </div>
    </div>
	<% include ../common/footer %>
</div>


</body>

    <% if(success!="") { %>
       <script type="text/javascript">
        $(document).ready(function(){

                demo.initChartist();

                $.notify({
                icon: 'fa fa-exclamation-circle',
                message:"<b><%= success %></b>"

            },{
                type: 'info',
                timer: 500
            });

        });
        </script>
    <%}%>

    <% if(error!="") { %>
      <script type="text/javascript">
      $(document).ready(function(){
      demo.initChartist();
      $.notify({
      icon: 'fa fa-exclamation-circle',
      message:"<b><%= error %></b>"
      },{
         type: 'danger',
         timer: 500
      });
      });
      </script>
   <%}%>
</html>
