<div class="main-panel">
<div class="container-fluid">
	<% include ../common/userBar %>
<br>

   <div class="content">
      <div class="container-fluid">
         <div class="row">
	    <div class="col">
	       <div class="card">
                  <div class="header2row">
                     <nav class="navbar navbar-inverse">
                        <ul class="nav navbar-nav" style="color:#669999">
			<li><a href="/cmsModule/cms/cmsUploadAdmin">Upload Documents</a></li>
			<li><a href="/cmsModule/cms/cmsApprAdmin">Approval Pending</a></li>
			<li><a href="/cmsModule/cms/cmsViewAdmin"><span style="color:#ff6600">View Documents</span></a></li>
			<% if(emp_access == "A1"){%>
				<li><a href="/cmsModule/cms/policyUploadAdmin">Upload Policy Docs</a></li>
			<% } %>
			<li><a href="/cmsModule/cms/policyViewAdmin">View Policy Docs</a></li>
			<% if(emp_access == "A1"){%>
                                <li><a href="/cmsModule/cms/magazineUploadAdmin">Upload Magazines</a></li>
                        <% } %>
			<li><a href="/cmsModule/cms/magazineViewAdmin">View Magazines</a></li>
			<li><a href="/cmsModule/cmsFAQDetails/cmsFAQDetails">FAQ</a></li>
			</ul>
		     </nav>
		  </div>
		  <div class="content">
			<hr/>
				<h4 class="title">View Documents     <i class="fa fa-file" aria-hidden="true"></i></h4>
			<hr/>
			 <% if(govLen != 0 || eduLen != 0 || medLen != 0 || expLen != 0 || phLen != 0 || resLen != 0 || othrLen != 0 || hrLen != 0 || cerLen != 0 || bgLen != 0) {%>
				<div class="row">
                                   <div class="col-md-5">
                                      <div class="form-group">
                                         <label style="color:#404040;">Employee</label>
					<input type="text" id="emp" name="emp" readOnly=true class="form-control" value="<%=employee[0].emp_name%> - <%=empId%>"/>
                                      </div>
                                   </div>
				   <div class="col-md-2" >
                                        <label style="color:#404040;"></label><br>
                                        <div class="form-group">
					<a class="btn btnLink" title="Back To Previous Page" href='/cmsModule/cms/cmsViewAdmin'>
                                        <i class="fa fa-arrow-left" aria-hidden="true"></i>
                                        </a>
                                        </div>
                                   </div>
                                </div>
			    <div class="row">
			     <% if(govLen != 0 || eduLen != 0 || medLen != 0 || expLen != 0 || phLen != 0) {%>
                              <div class="col-sm-6">
				<% if(govLen != 0) {%>
				  <label style="color:#404040;"><u>Government Documents</u></label>
				  <ol>
				      <% for (i = 0; i < govLen; i++)  {%>
					<li><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=govDocs[i]%>&empId=<%=empId%>" target="_blank"><%=govDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>

				<% if(eduLen != 0) {%>
				  <label style="color:#404040;"><u>Educational Documents</u></label>
				  <ol>
				      <% for (i = 0; i < eduLen; i++)  {%>
					<li><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=eduDocs[i]%>&empId=<%=empId%>" target="_blank"><%=eduDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>
				
				<% if(medLen != 0) {%>
				  <label style="color:#404040;"><u>Medical Document</u></label>
				  <ol>
				      <% for (i = 0; i < medLen; i++)  {%>
					<li style="color:#404040;"><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=medDocs[i]%>&empId=<%=empId%>" target="_blank"><%=medDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>

				<% if(expLen != 0) {%>
				  <label style="color:#404040;"><u>Experience Documents</u></label>
				  <ol>
				      <% for (i = 0; i < expLen; i++)  {%>
					<li style="color:#404040;"><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=expDocs[i]%>&empId=<%=empId%>" target="_blank"><%=expDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>

				<% if(phLen != 0) {%>
				  <label style="color:#404040;"><u>Photo</u></label>
				  <ol>
				      <% for (i = 0; i < phLen; i++)  {%>
					<li style="color:#404040;"><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=phDocs[i]%>&empId=<%=empId%>" target="_blank"><%=phDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>
			   </div>
			   <% } %>
		
			   <% if(resLen != 0 || othrLen != 0 || hrLen != 0 || cerLen != 0 || bgLen != 0) {%>
			   <div class="col-sm-6" >
				<% if(resLen != 0) {%>
                                  <label style="color:#404040;"><u>Resume</u></label>
                                  <ol>
                                      <% for (i = 0; i < resLen; i++)  {%>
					<li style="color:#404040;"><span style="width:100%; word-wrap:break-word; display:inline-block;">
                                        <a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=resDocs[i]%>&empId=<%=empId%>" target="_blank"><%=resDocs[i]%></a>
					</span></li>
                                      <% }; %>
                                  </ol>
                                  <br/>
                                <% } %>

				<% if(cerLen != 0) {%>
				  <label style="color:#404040;"><u>Certificates</u></label>
				  <ol>
				      <% for (i = 0; i < cerLen; i++)  {%>
					<li><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesEmployee?id=<%=cerDocs[i]%>&empId=<%=empId%>" target="_blank"><%=cerDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>

				<% if(othrLen != 0) {%>
				  <label style="color:#404040;"><u>Other Documents</u></label>
				  <ol>
				      <% for (i = 0; i < othrLen; i++)  {%>
					<li><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesEmployee?id=<%=othrDocs[i]%>&empId=<%=empId%>" target="_blank"><%=othrDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>

				<% if(bgLen != 0) {%>
				  <label style="color:#404040;"><u>Background Verification Document</u></label>
				  <ol>
				      <% for (i = 0; i < bgLen; i++)  {%>
					<li><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesEmployee?id=<%=bgDocs[i]%>&empId=<%=empId%>" target="_blank"><%=bgDocs[i]%></a>
					</span></li>
				      <% }; %>
				  </ol>
				  <br/>
				<% } %>

				<% if(hrLen != 0) {%>
                                  <label style="color:#404040;"><u>HR Upload Documents</u></label>
                                  <ol>
                                      <% for (i = 0; i < hrLen; i++)  {%>
					<li style="color:#404040;"><span style="width:100%; word-wrap:break-word; display:inline-block;">
					<a href="/cmsModule/cms/cmsViewFilesAdmin?id=<%=hrDocs[i]%>&empId=<%=empId%>" target="_blank"><%=hrDocs[i]%></a>
					</span></li>
                                      <% }; %>
                                  </ol>
                                  <br/>
                                <% } %>
			   </div>
			   <% } %>
		 	</div>

			<% } else { %>
				<label style="color:#404040;">No records found </label>
				<br>
			<% } %>
			<div class="clearfix"></div>
		  </div>
               </div>
           </div>
        </div>
     </div>
  </div>

    </div>
	<% include ../common/footer %>
</div>


</body>

    <% if(success!="") { %>
       <script type="text/javascript">
        $(document).ready(function(){

                demo.initChartist();

                $.notify({
                icon: 'fa fa-exclamation-circle',
                message:"<b><%= success %></b>"

            },{
                type: 'info',
                timer: 500
            });

        });
        </script>
    <%}%>

    <% if(error!="") { %>
      <script type="text/javascript">
      $(document).ready(function(){
      demo.initChartist();
      $.notify({
      icon: 'fa fa-exclamation-circle',
      message:"<b><%= error %></b>"
      },{
         type: 'danger',
         timer: 500
      });
      });
      </script>
   <%}%>
</html>
